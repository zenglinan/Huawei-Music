ources":{"src\\scss\\index.scss":"$color-p: #868AAF;\r\n$color: white;\r\n$background: #060a3d;\r\n@keyframes rotate {\r\n  from {\r\n    transform: rotate(0);\r\n  }\r\n  to {\r\n    transform: rotate(360deg);\r\n  }\r\n}\r\n#player {\r\n  background-color: $background;\r\n  height: 100vh;\r\n  width: 100vw;\r\n  display: flex;\r\n  flex-direction: column;\r\n  overflow: hidden;\r\n  header {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    .searchSong {\r\n      display: flex;\r\n      justify-content: center;\r\n      align-items: center;\r\n      padding-top: 12px;\r\n      width: 100vw;\r\n      img {\r\n        width: 3.5rem;\r\n        height: 3.5rem;\r\n        border-radius: 50%;\r\n        margin-right: 3rem;\r\n      }\r\n      #searchInput {\r\n        color: $color;\r\n        border: none;\r\n        outline: none;\r\n        width:68vw;\r\n        height: 3.5rem;\r\n        background-color: rgb(80, 83, 119);\r\n        border-radius: 4px;\r\n        padding: .5em;\r\n        font-size: 4rem;\r\n        margin-right: 1rem;\r\n      }\r\n      #searchBtn {\r\n        color: $color;\r\n      }\r\n    }\r\n    h1 {\r\n      color: $color;\r\n      padding: 20px 0 14px 0;\r\n    }\r\n    p {\r\n      color: $color-p;\r\n    }\r\n    .changePage {\r\n      display:flex;\r\n      justify-content: center;\r\n      align-items: center;\r\n      padding: 24px 0;\r\n      span {\r\n        margin: 0 5px;\r\n        display: block;\r\n        width: 6px;\r\n        height: 6px;\r\n        border-radius: 50%;\r\n        background-color: #6a134f;\r\n        &.active {\r\n          width: 10px;\r\n          height: 10px;\r\n          background-color: #db3baa;\r\n        }\r\n      }\r\n      \r\n    }\r\n  }\r\n  main {\r\n    width: 200vw;\r\n    flex-grow: 1;\r\n    display: flex;\r\n    .pageOne {\r\n      width: 100vw;\r\n      display: flex;\r\n      flex-direction: column;\r\n      flex-grow: 1;\r\n      .effect {\r\n        width: 100vw;\r\n        flex-direction: column;\r\n        display: flex;\r\n        flex-grow: 1;\r\n        justify-content: center;\r\n        align-items: center;\r\n        position: relative;\r\n        overflow: hidden;\r\n        >div {\r\n          position: absolute;\r\n        }\r\n        .effect1 {\r\n          background: url(../svg/effect-no-move.svg) 0 0 no-repeat;\r\n          width: 70vw;\r\n          height: 70vw;\r\n          animation: rotate 20s linear infinite;\r\n        }\r\n        .effect2 {\r\n          background: url(../svg/effect-move1.svg) 0 0 no-repeat;\r\n          width: 60vw;\r\n          height: 60vw;\r\n          animation: rotate 10s linear infinite reverse;\r\n        }\r\n        .effect3 {\r\n          background: url(../svg/effect-move2.svg) 0 0 no-repeat;\r\n          width: 24vw;\r\n          height: 24vw;\r\n          animation: rotate 10s linear infinite;\r\n        }\r\n      }\r\n      article.lyric {\r\n        text-align: center;\r\n        p {\r\n          line-height: 1.6;\r\n          color: $color-p;\r\n          &.active {\r\n            color: $color;\r\n            font-size: 4.5rem;\r\n          }\r\n        }\r\n      }\r\n    }\r\n    .pageTwo {\r\n      width: 100vw;\r\n      background-color: red;\r\n    }\r\n    \r\n  }\r\n  footer {\r\n    .controls {\r\n      display: flex;\r\n      justify-content: center;\r\n      padding: 16px 0;\r\n      svg {\r\n        width: 4rem;\r\n        height: 4rem;\r\n        margin: 0 20px;\r\n      }\r\n    }\r\n    .bar {\r\n      display: flex;\r\n      justify-content: space-around;\r\n      align-items: center;\r\n      span {\r\n        color: $color-p;\r\n        &.begin {\r\n          padding-left: 10px;\r\n        }\r\n        &.end {\r\n          padding-right: 10px;\r\n        }\r\n      }\r\n      .barWrapper {\r\n        display: flex;\r\n        margin: 0 8px;\r\n        position: relative;\r\n        align-items: center;\r\n        .point {\r\n          position: absolute;\r\n          background: url(../svg/progress.svg) no-repeat;\r\n          background-size: contain;\r\n          width: 16px;\r\n          height: 16px;\r\n          z-index: 1;\r\n        }\r\n        .barContent {\r\n          border-radius: 3px;\r\n          width: 65vw;\r\n          height: 4px;\r\n          background-color: #0025fe;\r\n        }\r\n      }\r\n      \r\n    }\r\n    .buttons {\r\n      align-items: center;\r\n      padding: 20px;\r\n      display: flex;\r\n      justify-content: space-around;\r\n      svg {\r\n        width: 30px;\r\n        height: 30px;\r\n        &.pausing,&.playing {\r\n          width: 60px;\r\n          height: 60px;\r\n        }\r\n        &.controlSong {\r\n          width: 45px;\r\n          height: 45px;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"3d4d39a4216fd75aaa4ccee3f9694c04","cacheData":{"env":{}}}